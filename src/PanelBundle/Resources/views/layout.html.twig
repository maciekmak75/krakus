            {% endif %}

        });
        $('.display').on('page.dt', function () {
            $('html, body').animate({
                scrollTop: $(".dataTables_wrapper").offset().top
            }, 'slow');
        });
        {% endif %}

        TableManageButtons.init();
        {% if app.request.attributes.get('_route')=='ksiazkaAdresowa' %}
        //load_data();


        function load_data(firstLastName) {
            NProgress.start()
            $.ajax({
                url: "{{ path('ksiazkaAdresowaAjax') }}",
                method: "POST",
                dataType: 'json',
                timeout: 7000,
                data: {firstLastName: firstLastName},
                beforeSend: function () {

                },
                success: function (response) {
                    $('#result').html(response.personInfoHtml);
