                    if (response.personInfoHtml == '') {
                        $('#result').text('Not found!');
                    }
                    var heights = $(".profile_view").map(function () {
                            return $(this).height();
                        }).get(),

                        maxHeight = Math.max.apply(null, heights);
                    $(".profile_view").height(maxHeight);
                    //$('#result').html(data);
                },
                error: function () { // if error
                    $('#result').html('Problem!');
